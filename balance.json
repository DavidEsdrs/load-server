{
  "balancing": {
    "type": "least_used"
  },
  "backends": [
    {
      "name": "main",
      "path": "localhost:3000",
      "rate_limit": {
        "type": "leaky_bucket",
        "generation_time_ms": 3000,
        "max_token": 1000
      }
    },
    {
      "name": "secondary",
      "path": "localhost:3001"
    },
    {
      "name": "tertiary",
      "path": "localhost:3002"
    }
  ]
}